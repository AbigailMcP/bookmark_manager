<h1> Links </h1>

<ul id='links'>
  <% @links.each do |link| %>
    <li>
      Title: <%= link.title %>
      URL:   <%= link.url   %>
    </li>
  <% end %>
</ul>

<form action="/sessions/end" method="post">

  <input type='submit' value='sign out'>
  <input type='hidden' name='_method' value='delete'>

</form>
